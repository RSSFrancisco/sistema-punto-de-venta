<Window x:Class="Presentacion.Consultas.empleadoEntre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Entre numeros" Height="200" Width="300" ShowInTaskbar="False" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="El menor" Height="28" HorizontalAlignment="Left" Margin="12,26,0,0" Name="lblMenor" VerticalAlignment="Top" />
        <Label Content="El mayor" Height="28" HorizontalAlignment="Left" Margin="12,71,0,0" Name="lblMayor" VerticalAlignment="Top" />
        <TextBox Height="23" MaxLength="4" HorizontalAlignment="Left" Margin="75,31,0,0" Name="txtMenor" VerticalAlignment="Top" Width="179" />
        <TextBox Height="23" MaxLength="6" HorizontalAlignment="Left" Margin="75,71,0,0" Name="txtMayor" VerticalAlignment="Top" Width="179" />
        <Button Content="Aceptar" Height="23" HorizontalAlignment="Left" Margin="19,126,0,0" Name="btnAceptar" VerticalAlignment="Top" Width="75" Click="btnAceptar_Click" />
        <Button Content="Cancelar" Height="23" HorizontalAlignment="Left" Margin="179,126,0,0" Name="btnCancelar" VerticalAlignment="Top" Width="75" Click="btnCancelar_Click" />
    </Grid>
</Window>
